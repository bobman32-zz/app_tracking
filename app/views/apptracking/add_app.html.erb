<form class="form-horizontal" action="/add_android" >
  <div class="form-group">
    <label for="androidapp" class="col-sm-2 control-label">Android App ID</label>
    <div class="col-sm-4">
      <input type="text" name="androidid" class="form-control" id="androidid" placeholder="ex. com.orbitz">

      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div>
</form>



<form class="form-horizontal" action="/add_ios">
  <div class="form-group">
    <label for="iosapp" class="col-sm-2 control-label">iOS App ID</label>
    <div class="col-sm-4">
      <input type="text" name="iosis" class="form-control"  id="iosid" placeholder="ex. 3242354325">


      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div>
</form>
